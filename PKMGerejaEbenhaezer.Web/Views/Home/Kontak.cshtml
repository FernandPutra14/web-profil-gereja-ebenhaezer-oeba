@{
    ViewData["Title"] = "Kontak";
}

<section id="kontak-us" class="mb-lg-5">
    <h2>
        Kontak /
    </h2>
</section>

<section id="contact">
    <div class="getin">
        <h2>
            Hubungi Kami
        </h2>
        <p>
            Butuh bantuan? Isi formulir ini dan mulai perjalanan rohani Anda bersama kami.
        </p>

        <div class="getin-details">
            <h3>Alamat</h3>
            <div>
                <i class="ri-home-8-line dpt"></i>
                <p>Jln. Ahmad Yani, Oeba, Kupang, Nusa tenggara Timur, Indonesia</p>
            </div>
            <h3>Telepon</h3>
            <div>
                <i class="ri-phone-line dpt"></i>
                <p>
                    +62 2123 4567 89 <br /> +62 2123 4567 89
                </p>
            </div>
            <h3>Bantuan</h3>
            <div>
                <i class="ri-mail-send-line dpt"></i>
                <p>example@gmail.com <br />example@gmail.com</p>
            </div>
            <h3>Profile</h3>
            <div class="pro-links">
                <i class="ri-instagram-fill"></i>
                <i class="ri-tiktok-fill"></i>
                <i class="ri-youtube-fill"></i>
                <i class="ri-twitter-x-fill"></i>
            </div>
        </div>
    </div>
    <div class="formko">
        <h4> Lets Connect</h4>
        <p>Bagaimana kami bisa membantu Anda?</p>
        <div class="form-row">
            <input type="text" placeholder="Nama Kamu" />
            <input type="text" placeholder="Email Kamu" />
        </div>
        <div class="form-col">
            <input type="text" placeholder="Subject" />
        </div>
        <div class="form-col">
            <textarea name="" id="" cols="30" rows="8" placeholder="Apa yang bisa kami bantu?"></textarea>
        </div>
        <div class="form-col">
            <button>Kirim Pesan</button>
        </div>
    </div>
</section>

<section id="peta" class="mb-5">
    <iframe src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d1963.6344805664785!2d123.58869978846019!3d-10.158779147488692!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x2c569ceb4e668f27%3A0x59169bcbf349bb9f!2sGMIT%20JEO%20KUPANG%20NTT!5e0!3m2!1sid!2sid!4v1719284911537!5m2!1sid!2sid" 
    style="border:0;" allowfullscreen="" loading="lazy" referrerpolicy="no-referrer-when-downgrade"></iframe>
</section>


<script type="text/javascript">
    $(function () {
        $('.badan-utama').css({
            'padding-left': '0',
            'padding-right': '0',
            'padding-top': '0rem',
        });
    });
</script>
